<h1>New Photo</h1>
<%= form_for( @photo, :url=>{:action=>'create'}, :html=>{:multipart=>true}) do |form| %>
 <div class="field">
  <%= form.label :image_file %><br>
<%= form.file_field :image_file %>
<br>

</div>
 <div class="field">
  <%= form.label :description %><br>
<%= form.text_field :description %><br>
</div>
 <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', photos_path %>
