

   <div class = "  well">
<table class="table table-striped"  >
  
  <thead>
    <tr>
      <h4>Notifications</h4>
      
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @notifications.order(created_at: :desc).each do |notification| %>
      <tr>
        <td><div class="text__"><%= notification.message %></div></td>
            </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<div class = "post_align0">
<%= paginate @notifications %>
</div>

